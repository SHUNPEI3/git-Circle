<div class="container pb-5">
  <div class="row justify-content-end my-4">
    <div class="col-lg-6 col-md-8 col-sm-12">
      <%= render 'public/searches/user_search_form' %>
    </div>
  </div>
  <div class="row m-4">
    <h3><i class="fas fa-tag"></i>＃タグが&nbsp;"<%=@tag.name%>"&nbsp;のユーザーリスト</h3>
  </div>
  <div class="row m-2">
    <%= render 'public/personal_tags/personal_tag_list', tag_list: @tag_list %>
  </div>
  <div class="row mt-2">
    <%= render 'public/end_users/user_list', end_users: @end_users %>
  </div>
  <%= paginate @end_users %>
</div>