<div class="container py-5">
  <div class="row justify-content-end">
    <div class="col-lg-6 col-md-8 col-sm-12">
      <%= render 'public/searches/user_search_form' %>
    </div>
  </div>
  <div class="row m-4">
    <h3><i class="fas fa-tag"></i>タグが&nbsp;"<%=@tag.name%>"&nbsp;のユーザーリスト</h3>
  </div>
  <div class="row m-2">
    <div class="col-12 mb-2">最近追加されたタグ</div>
    <div id="personal_tags"><%= render 'public/personal_tags/personal_tag_list', tag_list: @tag_list %></div>
    <div class="col-12 text-center my-1"><%= link_to_next_page @tag_list, '▽ もっと見る', remote: true, id: 'more_link' ,class:"text-secondary" %></div>
  </div>
  <div class="row justify-content-sm-start justify-content-around mt-2">
    <%= render 'public/end_users/user_list_normal', end_users: @end_users %>
  </div>
  <%= paginate @end_users %>
</div>