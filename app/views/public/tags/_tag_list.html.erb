<div class="col-12 mb-2">最近追加されたタグ</div>
<% tag_list.each do |list|%>
  <% unless list.communitys.count == 0 %>
    <span class="border rounded-pill bg-white mr-3 mb-2 py-2 px-3">
      <%=link_to "#{ "#" + list.name}", tag_path(list) %><%="(#{list.communitys.count})" %>
    </span>
  <% end %>
<% end %>