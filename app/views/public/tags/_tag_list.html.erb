<% tag_list.each do |list|%>
  <% unless list.communitys.count == 0 %>
  <div class="d-inline-block border rounded-pill bg-white mb-2 py-1 px-3">
    <span class="text-secondary"><%= link_to "#{ "#" + list.name}", tag_path(list), class:"text-secondary" %><%="(#{list.communitys.count})" %></span>
  </div>
  <% end %>
<% end %>