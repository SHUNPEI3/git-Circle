<div class="container py-5">
  <div class="row m-4">
    <h3>コミュニティ一覧</h3>
  </div>
  <div class="table-responsive-md">
    <table class="table text-nowrap">
      <thead class="thead-light">
        <th>ComuID</th>
        <th>コミュニティ名</th>
        <th>作成者</th>
        <th>メンバー</th>
        <th>参加条件設定</th>
        <th>募集状況</th>
        <th></th>
      </thead>
      <tbody>
        <%= render 'admin/communities/list', communities: @communities %>
      </tbody>
    </table>
  </div>
  <%= paginate @communities %>
</div>
